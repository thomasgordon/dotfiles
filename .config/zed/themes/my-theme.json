          "namespace": { "color": "#f7f6f2", "font_style": "italic" },
          "module": { "color": "#f7f6f2" },

          "primary": { "color": "#f7f6f2" },
          "emphasis": { "color": "#ffc2c6", "font_style": "italic" },
