          "label": { "color": "#c9707a" },

          "namespace": { "color": "#20201d", "font_style": "italic" },
          "module": { "color": "#20201d" },

          "primary": { "color": "#20201d" },
          "emphasis": { "color": "#c9707a", "font_style": "italic" },
          "emphasis.strong": { "color": "#c9707a", "font_weight": 700 },
          "link_text": { "color": "#27a855" },
          "link_uri": { "color": "#4a9db0" },
          "title": { "color": "#20201d", "font_weight": 700 }
