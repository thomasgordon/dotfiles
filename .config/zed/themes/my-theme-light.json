          "function.method": { "color": "#27a855" },
          "function.decorator": { "color": "#c9707a", "font_style": "italic" },

          "constructor": { "color": "#c9707a" },

          "variable": { "color": "#20201d" },
          "variable.builtin": { "color": "#c9707a", "font_style": "italic" },
          "variable.member": { "color": "#20201d" },
          "variable.special": { "color": "#c9707a" },

          "property": { "color": "#20201d" },

          "constant": { "color": "#c9707a" },
          "constant.builtin": { "color": "#c9707a", "font_style": "italic" },
          "constant.numeric": { "color": "#c9707a" },

          "string": { "color": "#4a9db0" },
          "string.special": { "color": "#27a855" },
          "string.escape": { "color": "#d43d47" },
